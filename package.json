  "homepage": "https://github.com/brookcs3/pleco-xa#readme",
  "bugs": {
    "url": "https://github.com/brookcs3/pleco-xa/issues"
  },
  "private": false,
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "http-server": "^14.1.1",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "rollup": "^4.0.0",
    "ts-jest": "^29.3.4",
    "web-audio-test-api": "file:./web-audio-test-api"
